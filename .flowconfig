[version]
0.69.0

[ignore]
<PROJECT_ROOT>/dashboards/.*
<PROJECT_ROOT>/data/.*
.*/node_modules/jss/.*.js.flow
.*/node_modules/redux-persist/.*.js.flow
.*/node_modules/immutable/dist/immutable.js.flow
<PROJECT_ROOT>/packages/.*/build/.*
.*/node_modules/protobufjs/src/bower.json
.*/node_modules/protobufjs/tests/data/invalid.json
.*/node_modules/protobufjs/tests/data/invalid.json
.*/node_modules/react-tap-event-plugin/src/TapEventPlugin.js
.*/node_modules/material-ui/.*.js.flow

[include]

[libs]
decls/

[lints]
all=error
sketchy-null-bool=off
untyped-import=warn
untyped-type-import=warn
unclear-type=off
unsafe-getters-setters=off

[options]
module.use_strict=true
module.file_ext=.mjs
module.file_ext=.js

esproposal.export_star_as=enable
experimental.const_params=true
