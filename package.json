    "packages/neotracker-server-web",
    "packages/neotracker-shared-graphql",
    "packages/neotracker-shared-test",
    "packages/neotracker-shared-utils",
    "packages/neotracker-shared-web",
    "packages/neotracker-shared-web-next"
