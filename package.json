  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && echo \"Executing tsc...\" && yarn run tsc && yarn run flow"
    }
  },
  "license": "MIT",
  "lint-staged": {
    "linters": {
      "packages/*/src/**/*.js": [
        "yarn run prettier --write",
        "git add",
        "yarn run lint:js"
      ],
      "packages/*/src/**/*.{ts,tsx}": [
        "yarn run prettier --write",
        "git add",
        "yarn run test:unit --findRelatedTests --ci",
        "yarn run test:browser --findRelatedTests --ci",
        "yarn run lint:ts"
      ]
    }
  },
  "name": "neotracker",
  "private": true,
  "repository": "https://github.com/neotracker/neotracker",
  "resolutions": {
    "babel-core": "7.0.0-bridge.0",
    "babel-loader": "8.0.0-beta.4",
    "babylon": "7.0.0-beta.47",
    "bn.js": "4.11.8",
    "elliptic": "6.4.1",
    "grpc": "1.14.1",
    "http-errors": "^1.7.0",
    "react": "16.4.2",
    "react-dom": "16.4.2",
    "regenerator-runtime": "^0.12.1",
    "ws": "^6.0.0"
  },
  "scripts": {
    "analyze": "yarn run ts-node ./packages/neotracker-build/src/entry/analyze.ts",
    "component-explorer": "yarn run ts-node ./packages/neotracker-component-explorer/src/node/entry/cli.ts",
    "develop": "yarn run ts-node ./packages/neotracker-build/src/develop.ts",
    "flow": "flow",
    "generate-gql-types": "apollo codegen:generate __generated__ --addTypename --schema=./packages/neotracker-server-graphql/src/__generated__/schema.graphql.json --target=typescript --queries='./packages/*/src/**/*.{ts,tsx}'",
    "jest": "jest --passWithNoTests",
    "jest:ci": "yarn run jest --ci -w 2",
    "lint": "yarn run lint:ts && yarn run lint:js:all && yarn run lint:css",
    "lint-staged": "lint-staged",
    "lint:css": "stylelint './packages/*/src/**/*.tsx'",
    "lint:js": "eslint",
    "lint:js:all": "yarn run lint:js \"packages/*/src/**/*.js\"",
    "lint:ts": "cross-env NODE_OPTIONS=\"--max-old-space-size=8192\" tslint --project . --format codeFrame",
    "nit": "yarn run nit:ts && yarn run nit:js",
    "nit:js": "prettier --list-different \"packages/*/src/**/*.js\"",
    "nit:ts": "prettier --list-different \"packages/*/src/**/*.ts\"",
    "prettier": "prettier --config .prettierrc",
    "prettier:all": "yarn run prettier:ts && yarn run prettier:js",
    "prettier:js": "yarn run prettier --write \"packages/*/src/**/*.js\"",
    "prettier:ts": "yarn run prettier --write \"packages/*/src/**/*.ts\"",
    "size": "size-limit",
    "test:browser": "yarn run jest --config jest/browser.js",
    "test:browser-ci": "yarn run jest:ci --config jest/browser-ci.js",
    "test:cypress": "yarn run ts-node ./packages/neotracker-server-test/src/cypress/run.ts",
    "test:cypress-ci": "yarn run test:cypress --ci",
    "test:e2e": "yarn run jest --config jest/e2e.js",
    "test:e2e-ci": "yarn run jest:ci --config jest/e2e-ci.js",
    "test:unit": "yarn run jest --config jest/unit.js",
    "test:unit-ci": "yarn run jest:ci --config jest/unit-ci.js",
    "ts-node": "cross-env NODE_OPTIONS=\"--max-old-space-size=8192\" TS_NODE_PROJECT=tsconfig/tsconfig.es2018.cjs.json node -r ts-node/register/transpile-only",
    "tsc": "tsc",
    "watch-gql-types": "yarn run ts-node ./packages/neotracker-build/src/entry/watchGraphQLTypes.ts"
  },
  "version": "0.0.0",
  "workspaces": [
    "packages/neotracker-build",
    "packages/neotracker-butterfly",
    "packages/neotracker-client-web",
    "packages/neotracker-client-web-next",
    "packages/neotracker-component-explorer",
    "packages/neotracker-core",
    "packages/neotracker-server-db",
    "packages/neotracker-server-graphql",
    "packages/neotracker-server-scrape",
